                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT    
DISTINCT
  I.COD_MP, I.NRO_MP, I.COD_BANCO, I.FECHA_MP,  I.COD_AUX, I.COD_COMP, T.ITEM, T.COD_DOC,
  T.NRO_DOC, T.FECHA_DOC, T.COD_CPTO, T.COD_CUENTA, T.GLOSA,  T.IMP_DOC, T.COD_D_H, T.COD_MONEDA,  T.COD_CC, 
  T.STATUS_TRANS,  MAESTRO_CONCEPTOS.DESC_CPTO, I.TIPO_OPERACION, 
   CASE WHEN I.COD_D_H = 'H' THEN (T .IMP_DOC * (- 1)) ELSE T .IMP_DOC END AS IMPORTE,  I.NRO_COMP, H.DESC_BANCO ,T.COD_PER,Z.DESC_PER
   FROM          I_BANCO AS I 
   INNER JOIN T_BANCO AS T ON I.COD_MP = T.COD_MP AND  I.NRO_MP = T.NRO_MP AND I.COD_BANCO = T.COD_BANCO 
   INNER JOIN MAESTRO_CONCEPTOS ON T.COD_CPTO = MAESTRO_CONCEPTOS.COD_CPTO  
   INNER JOIN MAESTRO_BANCOS H ON I.COD_BANCO=H.COD_BANCO 
   LEFT JOIN MAESTRO_PERSONAS Z ON T.COD_PER=Z.COD_PER
   WHERE  
    T.COD_MONEDA=@COD_MONEDA AND I.FECHA_MP BETWEEN @FECHA1 AND @FECHA2 AND (ISNULL(I.STATUS_EXT,'0')='0') AND T.COD_CPTO=@COD_CPTO  </CommandText>
